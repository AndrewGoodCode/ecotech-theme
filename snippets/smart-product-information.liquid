{% assign products = '' %}
{% assign variants = '' %}
{% assign str = '' %}
{% if product.metafields.custom.extend_bundle != blank %}
  {% assign extend_bundles = product.metafields.custom.extend_bundle.value %}
  {% for extend_bundle in extend_bundles %}
    {% assign products = extend_bundle.products.value %}
    {% assign variants = extend_bundle.variants.value %}
    {% for product_item in products %}
      {% assign metafields = product_item.metafields.custom %}
      {% for item in metafields %}
        {% assign temp = ',' | append: item[0] %}
        {% if str contains temp %}
        {% else %}
          {% assign str = str | append: ',' | append: item[0] %}
        {% endif %}
      {% endfor %}
    {% endfor %}
    {% assign prev = '' %}
    {% assign current = '' %}
    {% for variant_item in variants %}
      {% assign current = variant_item.product.handle %}
      {% if current != prev %}
        {% assign metafields = variant_item.product.metafields.custom %}
        {% for item in metafields %}
          {% assign temp = ',' | append: item[0] %}
          {% if str contains temp %}
          {% else %}
            {% assign str = str | append: ',' | append: item[0] %}
          {% endif %}
        {% endfor %}
      {% endif %}
      {% assign prev = current %}
    {% endfor %}
  {% endfor %}
{% endif %}

