      <div id="collapseYield" class="collapse" data-bs-parent="#accordion">
        <div class="card-body ps-4 ms-3">
          {% assign yield = product.metafields.custom.yield.value %}
          {% if yield.gallon_per_package.value != blank %}
            <div><span class="">Gallons per Package</span> - {{ yield.gallon_per_package.value }}</div>
          {% endif %}
          {% if yield.sqf_per_gallon.value != blank %}
            <div><span class="">Square Feet Coverage per Gallon </span> - {{ yield.sqf_per_gallon.value }}</div>
